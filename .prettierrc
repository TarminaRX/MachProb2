{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "xmlWhitespaceSensitivity": "ignore",
  "xmlSelfClosingSpace": true,
  "plugins": ["@prettier/plugin-xml"],
  "overrides": [
    {
      "files": "*.xml",
      "options": {
        "parser": "xml"
      }
    }
  ]
}
